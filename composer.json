{
    "name": "crthiago/laravel-services-actions",
    "description": "This package is designed to help developers generate the Services and Actions layer in their Laravel applications.\nIt provides a set of commands to generate the necessary files and folders, as well as a template for the Service and Action classes.",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Thiago Rodrigues Costa"
        }
    ],
    "require": {
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "nunomaduro/phpinsights": "^v2.8"
    },
    "autoload": {
        "psr-4": {
            "Crthiago\\LaravelServicesActions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crthiago\\LaravelServicesActions\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Crthiago\\LaravelServicesActions\\ServicesActionsProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
